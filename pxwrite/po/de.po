#: src/main.c:25
#, c-format
msgid "%s writes a paradox file."
msgstr "%s schreibt Paradox Dateien"

#: src/main.c:27
#, c-format
msgid "Usage: %s [OPTIONS] FILE"
msgstr "Aufruf: %s [OPTIONEN] Datei"

#: src/main.c:29
msgid "Options:"
msgstr "Optionen"

#: src/main.c:31
msgid "  -h, --help          this usage information."
msgstr "  -h, --help          diese Hilfe."

#: src/main.c:33
msgid "  -v, --verbose       be more verbose."
msgstr "  -v, --verbose       mehr Informationen anzeigen"

#: src/main.c:39
msgid "libpx uses librecode for recoding."
msgstr "libpx benutzt librecode zum reencodieren."

#: src/main.c:42
msgid "libpx uses iconv for recoding."
msgstr "libpx benutzt iconv zum reencodieren."

#: src/main.c:45
msgid "libpx has no support for recoding."
msgstr "libpx unterstützt nicht das reencodieren."

#: src/main.c:50
msgid "libpx has been compiled for big endian architecture."
msgstr "libpx wurde für Architekturen mit big endian übersetzt."

#: src/main.c:52
msgid "libpx has been compiled for little endian architecture."
msgstr "libpx wurde für Architekturen mit little endian übersetzt."

#: src/main.c:143
msgid "You must at least specify an output file."
msgstr "Sie müssen mindestens eine Ausgabedatei angeben."

#: src/main.c:155
msgid "Could not open input file."
msgstr "Konnte Eingabedatei nicht öffnen."

#: src/main.c:167 src/main.c:237
msgid "Could not create new paradox instance."
msgstr "Konnte neue Paradox-Instanz nicht erzeugen."

#: src/main.c:172
msgid "Could not get memory for field definitions."
msgstr "Konnte Speicher für Felddefinitionen nicht allozieren."

#: src/main.c:198 src/main.c:384
msgid "Could not create output file."
msgstr "Konnte Ausgabedatei nicht erzeugen."

#: src/main.c:218
msgid "Could not write record."
msgstr "Konnte Datensatz nicht schreiben."

#. Create a piece of memory where the pointers to the field values
#. * can be stored. This array of pointers is used for any row.
#. * csv_row_fread() initialized it to 0 before refilling it.
#.
#: src/main.c:246
msgid "Allocate memory for single line"
msgstr "Alloziere Speicher für eine Zeile."

#: src/main.c:247
msgid "Allocate memory for single row."
msgstr "Alloziere Speicher für eine Reihe."

#: src/main.c:265
msgid "Could not read first line of csv input."
msgstr "Konnte erste Zeile der CSV-Eingabe nicht lesen."

#. Read the field specification an build the field array for
#. * PX_create_fp()
#: src/main.c:272
msgid "Allocate memory for field definitions."
msgstr "Konnte Speicher für Felddefinitionen nicht allozieren."

#: src/main.c:273
msgid "Could not allocate memory for field specification array."
msgstr "Konnte Speicher für Array der Felddefinitionen nicht allozieren."

#: src/main.c:290
msgid ""
"Field specification must be a comma separated value (Name,Type,Size,Prec)."
msgstr ""
"Felddefinitionen müssen durch Komma getrennte Werte sein (Name,Typ,Größe,"
"Genauigkeit)"

#: src/main.c:295
msgid "Allocate memory for column name."
msgstr "Alloziere Speicher für Spaltenname."

#: src/main.c:296
#, c-format
msgid "Could not allocate memory for %d. field name."
msgstr "Konnte Speicher für %d. Feldnamen nicht allozieren."

#: src/main.c:306
#, c-format
msgid "%d. field specification ended unexpectetly."
msgstr "%d. Felddefinitionen unerwartet beendet."

#: src/main.c:311
#, c-format
msgid "%d. field specification misses type."
msgstr "Der %d. Felddefinitionen fehlt der Typ."

#: src/main.c:349
#, c-format
msgid "%d. field type '%c' is unknown."
msgstr "%d. Feldtyp '%c' ist unbekannt."

#: src/main.c:359 src/main.c:366
msgid "Field specification misses the column size."
msgstr "Der Felddefinitionen fehlt die Spaltengröße."

#: src/main.c:373
#, c-format
msgid ""
"The remainder '%s' of the specification for field %d is being disregarded."
msgstr "Der Rest '%s' der Felddefinition für Feld %d bleibt unbeachtet."

#: src/main.c:394
msgid "Allocate memory for record data."
msgstr "Alloziere Speicher für Datensatz."

#: src/main.c:395
msgid "Could not allocate memory for record data."
msgstr "Konnte Speicher für Datensatz nicht allozieren."

#: src/main.c:407
msgid "Could not read data line of csv input."
msgstr "Konnte Zeile der CSV-Eingabe nicht lesen."

#: src/main.c:419
#, c-format
msgid ""
"Input data in line %d has not the expected number of columns. Expected %d "
"got %d."
msgstr ""
"Eingabedaten in Zeile %d haben nicht die erwartete Anzahl Spalten. Erwartet "
"wurden %d, geliefert wurden %d."

